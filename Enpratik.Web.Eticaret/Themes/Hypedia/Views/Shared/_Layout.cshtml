    <meta name="keywords" content="@ViewBag.Keywords" />
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="stylesheet" type="text/css" media="screen" href="~/Themes/Hypedia/Content/css/normalize.css">
