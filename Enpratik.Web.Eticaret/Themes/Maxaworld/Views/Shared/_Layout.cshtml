<body class="@ViewBag.PageStyleClass page body_filled theme_skin_less article_style_stretch scheme_original top_panel_show top_panel_above sidebar_hide sidebar_outer_hide preloader wpb-js-composer sc_responsive">
    <div id="page_preloader"></div>
    
    <div class="body_wrap">
        <div class="row bg-primary" style="margin: 0 auto;padding: 5px 15px;">
            <div class="col-md-12 text-align-right" style="text-align: right;padding-right: 50px;">
                <a href="#" target="_blank" class="social_icons" style="padding-left:0; padding-right:0;">
                    <img src="/Themes/Maxaworld/Content/images/facebook.png" width="20" alt="facebook">
                </a>
                <a href="#" target="_blank" class="social_icons" style="padding-left:2px; padding-right:0">
                    <img src="/Themes/Maxaworld/Content/images/instagram.png" width="20" alt="instagram">
                </a>
            </div>
        </div>



        <div class="page_wrap">
            <div class="top_panel_fixed_wrap"></div>
            <div class="header_mobile">
                <div class="content_wrap">
                    <div class="menu_button icon-menu"></div>
                    <div class="logo">
                        <a href="@Url.Action("Index","Default")">
                            <img src="~/Themes/Maxaworld/Content/images/Logo.png" class="logo_main" alt="" width="238" height="56">
                        </a>
                    </div>
                </div>
                <div class="side_wrap">
                    <div class="close">Close</div>
                    <div class="panel_top">
                        <nav class="menu_main_nav_area">
                            <ul class="menu_main_nav">
                                @Html.Raw(menuHtml)
                            </ul>
                        </nav>
                        <div class="search_wrap search_style_regular search_state_fixed search_ajax">
                            <div class="search_form_wrap">
                                <form role="search" method="get" class="search_form" action="#">
                                    <button type="submit" class="search_submit icon-search-1" title="Start search"></button>
                                    <input type="text" class="search_field" placeholder="Search" value="" name="s" />
                                </form>
                            </div>
                            <div class="search_results widget_area scheme_original">
                                <a class="search_results_close icon-cancel"></a>
                                <div class="search_results_content"></div>
                            </div>
                        </div>
                    </div>
                    <div class="panel_bottom">
                    </div>
                </div>
                <div class="mask"></div>
            </div>
            <header class="top_panel_wrap top_panel_style_7 scheme_original">
                <div class="top_panel_wrap_inner top_panel_inner_style_7 top_panel_position_above">
                    <div class="top_panel_middle">
                        <div class="content_wrap">
                            <div class="column-1_3 contact_logo">
                                <div class="logo">
                                    <a href="@Url.Action("Index","Default")">
                                        <img src="~/Themes/Maxaworld/Content/images/Logo.png" class="logo_main" alt="" />
                                        <img src="~/Themes/Maxaworld/Content/images/Logo.png" class="logo_fixed" alt="" />
                                    </a>
                                </div>
                            </div>
                            <div class="column-2_3 menu_main_wrap">
                                <nav class="menu_main_nav_area">
                                    <ul class="menu_main_nav">
                                        @Html.Raw(menuHtml)
                                    </ul>
                                </nav>
                            </div>
                        </div>
                    </div>
                </div>
            </header>
            @RenderBody()


            <footer class="footer_wrap widget_area scheme_original">
                <div class="footer_wrap_inner widget_area_inner">
                    <div class="content_wrap">
                        <div class="columns_wrap">
                            <aside class="column-1_3 widget widget_socials">
                                <div class="widget_inner">
                                    <div class="logo_descr">
                                        We would love to hear from you!<br />
                                        If you have a question about us, our programs or prices, please fill the form. We will be happy to contact you and will help to find the best solutions for yourself.
                                    </div>
                                </div>
                            </aside>
                            <aside class="column-1_5 widget widget_text">
                                <h5 class="widget_title">Contacts</h5>
                                <div class="textwidget">
                                    <ul class="sc_list_style_iconed ">
                                        <li>
                                            <span class="sc_list_icon icon-icon_phone"></span><a href="tel:+902122117474">+90 212 211 74 74</a>
                                        </li>
                                        <li>
                                            <span class="sc_list_icon icon-icon_phone"></span><a href="tel:+905525290074">+90 552 529 00 74</a>
                                        </li>
                                        <li>
                                            <span class="sc_list_icon icon-icon_mail"></span>
                                            <a href="mailto:info@maxaworld.com">info@maxaworld.com</a>
                                        </li>
                                    </ul>
                                </div>
                            </aside>
                            <aside class="column-1_5 widget widget_recent_posts">
                                <h5 class="widget_title">Menu</h5>
                                <div class="textwidget">
                                    <ul class="sc_list_style_iconed ">
                                        @foreach (var item in menuParent)
                                        {
                                            <li style="padding:0!important;"><a href="@item.MenuUrl">@item.MenuName</a></li>
                                        }
                                    </ul>
                                </div>
                            </aside>
                            <aside class="column-1_4 widget">
                                <ul class="instagram-pics instagram-size-large">
                                    <li style="width: auto!important;">
                                        <a href="#" target="_blank" class="social_icons">
                                            <img src="~/Themes/Maxaworld/Content/images/whatsapp.png" style="width:45px;" alt="whatsapp" />
                                        </a>
                                    </li>
                                    <li style="width: auto!important;">
                                        <a href="#" target="_blank" class="social_icons">
                                            <img src="~/Themes/Maxaworld/Content/images/facebook.png" style="width:45px;" alt="facebook" />
                                        </a>
                                    </li>
                                    <li style="width: auto!important;">
                                        <a href="#" target="_blank" class="social_icons">
                                            <img src="~/Themes/Maxaworld/Content/images/instagram.png" style="width:45px;" alt="instagram" />
                                        </a>
                                    </li>
                                </ul>
                                <hr />
                                <div class="textwidget">
                                    <ul class="sc_list_style_iconed ">
                                        <li style="padding:0!important;">
                                            <a href="/terms-of-use">Terms of Use</a>
                                        </li>
                                        <li style="padding:0!important;">
                                            <a href="/privacy-policy">Privacy Policy</a>
                                        </li>
                                        <li style="padding:0!important;">
                                            <a href="/faq">FAQ</a>
                                        </li>
                                    </ul>
                                </div>
                            </aside>
                        </div>
                    </div>
                </div>
            </footer>

            <div class="copyright_wrap copyright_style_socials scheme_original">
                <div class="copyright_wrap_inner">
                    <div class="content_wrap">
                        <div class="copyright_text">
                            <p>
                                Maxaworld © 2019 | Powered by <a href="www.enpratik.net" target="_blank">enpratik.net</a>
                            </p>
                        </div>
                    </div>
                </div>
            </div>


        </div>
    </div>

    <a href="https://wa.me/905525290074" target="_blank">
