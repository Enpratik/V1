            <div class="row">
                <div class="col-md-offset-1 col-md-10">
                    <div class="process-box" id="subscription-1" data-current-section="choose-subscription" data-to-section="choose-additional-product">
                        <div class="process-box_check"><span></span></div>
                        <div class="process-box_wrapper">
                            <div class="process-box_content">
                                <p>HER AY</p>
                                <h6>119 TL / AY</h6>
                                <span class="d-block">&nbsp;</span>
                                <span class="caption-small"><span class="bold">her ay</span> kapında.</span>
                            </div>
                        </div>
                    </div>
                    <div class="process-box" id="subscription-2" data-current-section="choose-subscription" data-to-section="choose-additional-product">
                        <div class="process-box_check"><span></span></div>
                        <div class="process-box_wrapper">
                            <div class="process-box_content">
                                <p>3 AYLIK</p>
                                <h6>329 TL</h6>
                                <span class="d-block">(109 TL / AY)</span>
                                <span class="caption-small">3 ay peşin ödemeli, <span class="bold">her ay</span> kapında.</span>
                            </div>
                        </div>
                    </div>
                    <div class="process-box" id="subscription-3" data-current-section="choose-subscription" data-to-section="choose-additional-product">
                        <div class="process-box_check"><span></span></div>
                        <div class="process-box_wrapper">
                            <div class="process-box_content">
                                <p>6 AYLIK</p>
                                <h6>629 TL</h6>
                                <span class="d-block">(104 TL / AY)</span>
                                <span class="caption-small">6 ay peşin ödemeli, <span class="bold">her ay</span> kapında.</span>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>

